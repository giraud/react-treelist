{
  "name": "@giraud/react-treelist",
  "version": "1.2.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/@giraud/react-treelist.js",
  "main": "./dist/@giraud/react-treelist.js",
  "exports": {
    ".": {
      "import": "./dist/@giraud/react-treelist.js",
      "require": "./dist/@giraud/react-treelist.umd.cjs"
    }
  },
  "description": "A React treelist component to display data in tree structure.",
  "scripts": {
    "start": "vite dev",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint ./src",
    "format": "prettier src/lib --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maheshsenni/react-treelist.git"
  },
  "author": "Mahesh Senniappan <mahesh.h4evr@gmail.com>",
  "bugs": {
    "url": "https://github.com/maheshsenni/react-treelist/issues"
  },
  "homepage": "https://github.com/maheshsenni/react-treelist",
  "license": "MIT",
  "dependencies": {
    "dateformat": "^5.0.0",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "prop-types": "^15.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.3",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/demo/",
      "<rootDir>/src/sample-data/"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "keywords": [
    "tree",
    "treelist",
    "treegrid",
    "list",
    "grid",
    "react"
  ]
}
